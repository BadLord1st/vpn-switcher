# Listen address for HTTP API
LISTEN_ADDR=0.0.0.0:8080

# Persistent state file path
STATE_PATH=/var/lib/vpn-switcher/state.json

# Outline CLI wrapper command
OUTLINE_COMMAND_BIN=vpn

# Args to discover profiles automatically
OUTLINE_LIST_ARGS=list -f %name%

# Args prefix for switching, profile will be appended as the last argument
OUTLINE_COMMAND_PREFIX=connect

# Runtime worker threads; service clamps to 1..2 internally
TOKIO_WORKER_THREADS=2

# Optional rust logs, e.g. info,debug
RUST_LOG=info
